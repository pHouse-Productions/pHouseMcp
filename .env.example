# OpenRouter API Key (for image generation)
# Get one at https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google API Credentials
# Download OAuth credentials from Google Cloud Console
# Default locations: ~/.config/phouse/google_credentials.json and ~/.config/phouse/google_token.json
GOOGLE_CREDENTIALS_PATH=/path/to/client_secret.json
GOOGLE_TOKEN_PATH=/path/to/tokens.json

# Telegram Bot Token
# Get one from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Google Places API Key
# Enable Places API in Google Cloud Console
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Finnhub API Key (for stock quotes and company news)
# Get one at https://finnhub.io/register
FINNHUB_API_KEY=your_finnhub_api_key_here

# Discord Bot Token
# Create a bot at https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_discord_bot_token_here
